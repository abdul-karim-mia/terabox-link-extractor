{
  "name": "terabox-link-extractor",
  "version": "1.3.2",
  "homepage": "https://github.com/abdul-karim-mia/terabox-link-extractor",
  "description": "High-performance TeraBox direct link extractor using the XAPIverse protocol.",
  "author": "Abdul Karim Mia",
  "config": {
    "TERABOX_API_KEY": {
      "type": "string",
      "description": "XAPIverse API Key",
      "required": true
    },
    "API_KEY": {
      "type": "string",
      "description": "API Key (Fallback)",
      "required": false
    }
  },
  "metadata": {
    "openclaw": {
      "requires": {
        "bins": [
          "node"
        ],
        "env": [
          "TERABOX_API_KEY",
          "API_KEY"
        ],
        "config": []
      },
      "primaryEnv": "TERABOX_API_KEY"
    }
  }
}